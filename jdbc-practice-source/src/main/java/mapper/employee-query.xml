<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment/>
    <!-- 쿼리 작성 -->
    <entry key="selectEmpByFamilyName">
        SELECT
        E.*
        FROM EMPLOYEE E
        WHERE E.EMP_NAME LIKE CONCAT(?, '%')
    </entry>
    <entry key="deleteMenu">
        DELETE
        FROM EMPLOYEE
        WHERE EMP_NAME = ?
    </entry>
</properties>
