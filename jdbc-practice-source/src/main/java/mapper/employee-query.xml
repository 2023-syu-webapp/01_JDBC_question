<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment/>
<entry key="selectEmp">
    SELECT *
    FROM EMPLOYEE
    WHERE EMP_ID = ?
</entry>

    <entry key="insertEmp">
        INSERT INTO EMPLOYEE
        VALUES(?,?,?,?,?,?,?,?,?,?,?,?,?,?)
    </entry>

    <entry key="deleteEmp">
        delete
        from employee
        where emp_name = ?
    </entry>

    <entry key="updateEmp">
        UPDATE EMPLOYEE
        SET PHONE = ?,EMAIL = ?,DEPT_CODE = ?,SALARY = ?,BONUS = ?
        WHERE EMP_ID = ?
    </entry>

</properties>
